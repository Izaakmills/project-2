<div id="home">
  <div class="container-fluid">
    <div class="row">
      <div class="col-2">
        <img src="/images/TMT-logo.png" alt="TMT Logo" class="sticky-top">
        <img src="/images/street_logo.png" alt="Street logo">
        <img src="/images/StoneChurch_logo.png">
       <!-- <img src="/images/BritishBeer_logo.png" alt="British Beer Company logo"  class="fixedleft"> -->
      </div>
