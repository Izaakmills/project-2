        <h1 class="font-effect-wallpaper" style="font-size:65px; font-family:Shojumaru; color:black">Happy Hours Near You</h1>
        <br>

          <div id="monday" class="clearfix">
            <h2 class="font-effect-shadow-multiple" style="font-size:40px; font-family:Shojumaru;">Monday </h2>
            {{#each user}}
            <div class="card">
                <div class="card-header" id="headingOne">
                  <h5 class="mb-0">
                    <button class="btn btn-link" data-toggle="collapse" data-target="#{{this.monday}}" aria-expanded="false" aria-controls="{{this.monday}}" style="text-decoration:none;">
                       Name: {{this.business}} | Event: {{this.monday}}
                    </button>
                  </h5>
                </div>

                <div id="{{this.monday}}" class="collapse hide" aria-labelledby="headingOne" data-parent="#accordion">
                  <div class="card-body">
                    Location: {{this.address}} | {{this.state}} | {{this.city}} | {{this.zip}}
                  </div>
                </div>
              
            </div>
            {{/each}}

          </div>

